        <mxCell id="mgxl76x7QrMjUIGk2s-J-2" value="- Node.js MongoDB official Driver&#xa;&#xa;- CRUD:&#xa;   READ&#xa;   .find() =&gt; returns a curson&#xa;    vs&#xa;    SELECT * FROM `table_name`;&#xa;&#xa;&#xa;    CREATE&#xa;&#xa;    .insertOne() &amp; .insertMany()&#xa;    vs&#xa;    INSERT INTO `table_name` (col1, col2,...)&#xa;    VALUES (value1, value2, ...);&#xa;&#xa;    UPDATE&#xa;    .updateOne() &amp; .updateMany()&#xa;    UPDATE `table_name`&#xa;    SET col1 = value1, &#xa;    col2 = value2, &#xa;    col3 = value3&#xa;    WHERE column1 = &#39;&#39; AND column2 = &#39;&#39;;&#xa;&#xa;    DELETE&#xa;    .deleteOne()&#xa;    vs&#xa;    DELETE FROM `table_name`&#xa;    WHERE condition;&#xa;&#xa;    .deleteMany( {} );&#xa;&#xa;    vs&#xa;    BEGIN;&#xa;    DELETE FROM `table_name`;&#xa;    COMMIT;       -- Check if dbOp succeeds&#xa;&#xa;    ROLLBACK; -- rollback if sth fails&#xa;&#xa;" style="whiteSpace=wrap;align=center;verticalAlign=middle;fontStyle=1;strokeWidth=3;fillColor=#df8df0;align=left;" vertex="1" parent="1">
