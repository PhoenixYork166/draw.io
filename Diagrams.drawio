        <mxCell id="LWeolHifm1ltcingEa9k-161" value="const multer = require(&#39;multer&#39;);&#xa;const uuid = require(&#39;uuid/v4&#39;);&#xa;&#xa;// Mapping for Image File MIME Types&#xa;const MIME_TYPE_MAP = {&#xa;&#39;image/png&#39;: &#39;png&#39;,&#xa;&#39;image/jpeg&#39;: &#39;jpeg&#39;,&#xa;&#39;image/jpg&#39;: &#39;jpg&#xa;};&#xa;&#xa;// multer = a group of middlewares&#xa;const fileUpload = multer){&#xa;&#xa;limits: 2048000, // 2MB File Size limit&#xa;storage: multer.diskStorage({&#xa;destination: (req, file, cb) =&gt; {&#xa;cb(null, &#39;uploads/images&#39;);&#xa;&#xa;filename: (req, file, cb) =&gt; {&#xa;const ext = MIME_TYPE_MAP(file.mimetype);&#xa;const generatedFileName = uuid() + &#39;.&#39; + ext;&#xa;cb(null, generatedFileName);&#xa;&#xa;console.log(`\nGenerated file name: ${generatedFileName}`);&#xa;console.log(`\nStorage location: backend/uploads/images/${generatedFileName}\n`);&#xa;&#xa;}&#xa;}),&#xa;&#xa;fileFilter: (req, file, cb) =&gt; {&#xa;// Filter outby mapping mime type for file.ext&#xa;const isValid = !!MIME_TYPE_MAP[file.mimetype]; // boolean&#xa;&#xa;let error = isValid ? null : new Error(`\nInvalid MIME_TYPE for file!\n`);&#xa;&#xa;if (!isValid) {&#xa;console.log(`\nInvalid file extension for: ${file.originalname}`);&#xa;console.log(`\nAttempted MIME type: ${file.mimetype}\n`);&#xa;}&#xa;&#xa;cb(error, isValid);&#xa;}&#xa;});&#xa;&#xa;module.exports = fileUpload;" style="whiteSpace=wrap;align=center;verticalAlign=middle;fontStyle=1;strokeWidth=3;fillColor=#00FFFF" vertex="1" parent="1">
